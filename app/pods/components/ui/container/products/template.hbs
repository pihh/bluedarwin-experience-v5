<div class="bde__ui-container-footer {{this.container.$productClassList}}">
  {{#each this.container.productButtons as |button|}}
    <Ui::Button::Rounded
      @title={{button.title}}
      @icon={{button.icon}}
      @image={{button.image}}
      @actionName={{button.actionName}}
    />
    {{button.image}}
  {{/each}}
</div>